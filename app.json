{
  "expo": {
    "name": "Agua Lily Mobile",
    "slug": "agua-lily-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "lilycf",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.jdlsantos.agualilymobile"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": ["expo-secure-store"],
    "extra": {
      "eas": {
        "projectId": "b116acb7-d591-454b-8eac-946726569a80"
      }
    }
  }
}
